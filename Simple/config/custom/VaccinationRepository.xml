<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE gsa-template
	PUBLIC "-//Art Technology Group, Inc.//DTD Dynamo Security//EN"
	"http://www.atg.com/dtds/gsa/gsa_1.0.dtd">
<gsa-template>
	<header>
		<name>Vaccination Repository</name>
		<author>Julia Malyshko</author>
	</header>

	<item-descriptor name="vaccination" default="true">
		<table name="ATGTR_VACCINATION" type="primary" id-column-names="vaccination_id">
			<property name="vaccinationId" column-name="vaccination_id" data-type="String" />
			<property name="vaccinationNumber" column-name="vaccination_number" data-type="String" />
			<property name="vaccinationDate" column-name="vaccination_date" data-type="Date" />
		</table>

	</item-descriptor>
</gsa-template>

