<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE gsa-template
	PUBLIC "-//Art Technology Group, Inc.//DTD Dynamo Security//EN"
	"http://www.atg.com/dtds/gsa/gsa_1.0.dtd">
<gsa-template>
	<header>
		<name>Owner Repository</name>
		<author>Julia Malyshko</author>
	</header>

	<item-descriptor name="owner" default="true">
		<table name="ATGTR_OWNER" type="primary" id-column-name="owner_id">
			<property name="ownerId" column-name="owner_id" data-type="String" />
			<property name="ownerName" column-name="owner_name"
				data-type="String" required="true" />
			<property name="feline" column-name="feline_id" item-type="feline"
				repository="/custom/Feline" required="true" />
		</table>
	</item-descriptor>

</gsa-template>

