<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE gsa-template
	PUBLIC "-//Art Technology Group, Inc.//DTD Dynamo Security//EN"
	"http://www.atg.com/dtds/gsa/gsa_1.0.dtd">
<gsa-template>
	<header>
		<name>Prize Repository on CATA</name>
		<author>Julia Malyshko</author>
	</header>

	<item-descriptor name="prize" default="true">
		<table name="ATGTR_PRIZE" type="primary" id-column-names="prize_id">
			<property name="prizeId" column-name="prize_id" data-type="String" />
			<property name="place" column-name="place" data-type="int"
				required="true" />
			<property name="award" column-name="award" data-type="String" />
		</table>

	</item-descriptor>
</gsa-template>

